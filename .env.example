# Environment Variables for EcoSnap AI

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Gemini API
# Environment Configuration for Production Deployment
# Copy this to .env.local and fill in your API keys

# Gemini AI API Key (Required for product analysis and AR scanner)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Analytics and monitoring
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# Build optimization
VITE_BUILD_OPTIMIZE=true
VITE_BUILD_MINIFY=true

# Performance monitoring
VITE_ENABLE_ANALYTICS=true

# Feature flags for deployment
VITE_ENABLE_AR_SCANNER=true
VITE_ENABLE_BULK_SCAN=true
VITE_ENABLE_CAMERA_UPLOAD=true
VITE_ENABLE_BARCODE_SCANNER=true

# API Configuration
VITE_API_TIMEOUT=30000
VITE_MAX_FILE_SIZE=10485760

# Production optimizations
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true

# OpenFoodFacts API (Free - No key needed)
VITE_OPENFOODFACTS_URL=https://world.openfoodfacts.org

# Unsplash API for Images (Free tier)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Carbon Interface API (Free tier)
VITE_CARBON_INTERFACE_KEY=your_carbon_interface_key_here

# RecycleNation API (Free)
VITE_RECYCLENATION_KEY=your_recyclenation_key_here

# App Configuration
VITE_APP_NAME=EcoSnap AI
VITE_APP_VERSION=2.0.0
