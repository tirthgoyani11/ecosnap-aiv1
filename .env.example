# Environment Variables for EcoSnap AI

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Gemini API
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenFoodFacts API (Free - No key needed)
VITE_OPENFOODFACTS_URL=https://world.openfoodfacts.org

# Unsplash API for Images (Free tier)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Carbon Interface API (Free tier)
VITE_CARBON_INTERFACE_KEY=your_carbon_interface_key_here

# RecycleNation API (Free)
VITE_RECYCLENATION_KEY=your_recyclenation_key_here

# App Configuration
VITE_APP_NAME=EcoSnap AI
VITE_APP_VERSION=2.0.0
